{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@firebase": [
        "src/firebase/*"
      ],
      "@containers/*": [
        "src/containers/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@pexels/*": [
        "src/pexels/*"
      ],
      "@controllers/*": [
        "src/controllers/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@db/*": [
        "src/db/*"
      ]
    },
  }
}